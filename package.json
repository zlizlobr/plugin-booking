{
  "name": "Booking",
  "version": "1.0.0",
  "description": "refaktor a preact acf-flowform",
  "scripts": {
    "build": "npm run build:admin && npm run build:public && npm run build:blocks",
    "build:admin": "NODE_ENV=production BUILD_TARGET=admin webpack --config webpack.optimized.config.js --mode production",
    "build:public": "NODE_ENV=production BUILD_TARGET=public webpack --config webpack.optimized.config.js --mode production",
    "build:blocks": "NODE_ENV=production webpack --config webpack.blocks.config.js --mode production && npm run copy:blocks-css",
    "copy:blocks-css": "mkdir -p assets/css/dist/blocks && find assets/css/blocks -name '*.css' ! -name '*compiled*' ! -name '*final*' -exec cp {} assets/css/dist/blocks/ \\;",
    "build:legacy": "webpack --mode production",
    "dev": "npm run dev:admin & npm run dev:public & npm run dev:blocks",
    "dev:admin": "BUILD_TARGET=admin webpack --config webpack.optimized.config.js --mode development --watch",
    "dev:public": "BUILD_TARGET=public webpack --config webpack.optimized.config.js --mode development --watch",
    "dev:blocks": "webpack --config webpack.blocks.config.js --mode development --watch",
    "dev:legacy": "webpack --mode development --watch",
    "watch": "npm run dev",
    "clean": "rm -rf assets/css/dist/*.css assets/js/dist/*.js assets/js/dist/*.map",
    "analyze": "npm run build && du -h assets/css/dist/*.css assets/js/dist/*.js"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-react-jsx": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.9",
    "@babel/preset-typescript": "^7.27.1",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "@tailwindcss/cli": "^4.1.12",
    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "cssnano": "^7.1.1",
    "mini-css-extract-plugin": "^2.9.2",
    "null-loader": "^4.0.1",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.1.1",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.1",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "main": "postcss.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@preact/signals": "^1.3.2",
    "@preline/datepicker": "^3.2.3",
    "@wordpress/components": "^30.3.0",
    "@wordpress/hooks": "^4.0.0",
    "@wordpress/i18n": "^4.0.0",
    "clsx": "^2.0.0",
    "country-list-with-dial-code-and-flag": "^5.1.1",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "preact": "^10.27.1",
    "preline": "^3.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vanilla-calendar-pro": "^3.0.5"
  }
}
