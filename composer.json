{
        "name": "artevio/wpcbooking",
        "description": "Plugin for booking",
    "type": "wordpress-plugin",
    "license": "proprietary",
    "authors": [
        {
            "name": "Artevio",
            "email": "info@artevio.com",
            "homepage": "https://artevio.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
                "php": ">=7.4",
                "wpify/custom-fields": "^4.0"
        },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "keywords": [
        "wordpress",
        "woocommerce",
        "booking"
    ],
    "autoload": {
        "psr-4": {
            "Wpcbooking\\": "includes/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs": "phpcs --standard=PSR12 includes/",
        "cs-fix": "phpcbf --standard=PSR12 includes/",
        "post-install-cmd": [
            "composer dump-autoload --optimize"
        ],
        "post-update-cmd": [
            "composer dump-autoload --optimize"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
